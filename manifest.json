{
	"manifest_version": 2,
	"name": "batt",
	"version": "0.1",
	"description": "(ノ｀⌒´)ノ ┫ block all the things!",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"http://*/*",
		"https://*/*"
	]
}
